<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/run_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:onClick="full_supers"
        android:text="do supers"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b" />

    <TextView
        android:id="@+id/a"
        android:layout_width="172dp"
        android:layout_height="40dp"
        android:text="בשבילך בן"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/b"
        android:layout_width="171dp"
        android:layout_height="49dp"
        android:layout_marginTop="48dp"
        android:text="הכבוד שלך תלחץ"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/a" />

    <TextView
        android:id="@+id/c"
        android:layout_width="351dp"
        android:layout_height="67dp"
        android:text="לא ניסיתי עדיין אז אם לא עובד לא לוקח אחריות"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="444dp" />

    <Button
        android:id="@+id/prudbuttem"
        android:layout_width="200dp"
        android:layout_height="58dp"
        android:layout_marginTop="44dp"
        android:text="do products"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/run_button" />
</androidx.constraintlayout.widget.ConstraintLayout>